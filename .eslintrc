{
    "extends": "@react-native-community",
    "rules": {
        "quotes": [2, "single", { "avoidEscape": true }],
        "semi": [2, "never"]
    },
    "plugins": ["jsx", "flow"],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false,
        "plugins": ["jsx", "flow"],
        "babelOptions": {
            "presets": ["@babel/preset-react"]
        }
    }
}
